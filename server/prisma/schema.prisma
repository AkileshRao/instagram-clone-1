// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id         Int       @id @default(autoincrement())
  media_url  String
  name       String
  likes      Int
  comments   Int
  caption    String
  created_at DateTime? @default(now())
}
